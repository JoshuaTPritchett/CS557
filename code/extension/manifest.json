{
  "manifest_version": 2,

  "name": "Extension for Exposit",
  "description": "This extension will be used for code analysis ONLY",
  "version": "0.0",

  "page_action": {
    "default_icon": {
     "19":  "test_icon.png",
     "38":  "test_icon.png"
  },
    "default_popup": "exposit.html",
    "default_title": "Exposit"
  },

  "permissions": [
    "https://*.google.com/",
    "https://*google.com/*",
    "http://*google.com/",
    "http://*google.com/*",
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
